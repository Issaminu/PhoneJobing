{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "fruitcake/php-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.5.0",
        "jxlwqq/echarts": "^1.0.2",
        "laravel/framework": "^10.3.0",
        "laravel/sanctum": "^3.2.5",
        "laravel/tinker": "^2.8.1",
        "league/flysystem-aws-s3-v3": "^3.15",
        "league/mime-type-detection": "^1.11",
        "tonysm/rich-text-laravel": "^1.6.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12.3",
        "fakerphp/faker": "^1.20.0",
        "laravel/breeze": "^1.21.0",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^6.3.0",
        "spatie/laravel-ignition": "2.1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "composer --no-dev update",
            "chmod -R 777 public/",
            "php artisan schedule:work"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
